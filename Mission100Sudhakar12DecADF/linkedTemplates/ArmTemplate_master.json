{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Mission100Sudhakar12DecADF"
		},
		"ADLS_inked_Serive_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_inked_Serive'"
		},
		"Misson100_AzureSqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Misson100_AzureSqlDB'"
		},
		"ADLS_inked_Serive_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sudhakar12dec89adls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Mission100Sudhakar12DecADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_inked_Serive_accountKey": {
						"value": "[parameters('ADLS_inked_Serive_accountKey')]"
					},
					"Misson100_AzureSqlDB_connectionString": {
						"value": "[parameters('Misson100_AzureSqlDB_connectionString')]"
					},
					"ADLS_inked_Serive_properties_typeProperties_url": {
						"value": "[parameters('ADLS_inked_Serive_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}