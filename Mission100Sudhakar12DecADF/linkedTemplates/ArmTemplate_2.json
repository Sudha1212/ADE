								"activity": "Get HighWater mark",
								"dependencyConditions": [
									"Succeeded"
								]
							},
							{
								"activity": "LastUpdated Date",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
